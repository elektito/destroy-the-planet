; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "Global",
"language": "GDScript",
"path": "res://Global.gd"
} ]
_global_script_class_icons={
"Global": ""
}

[application]

config/name="Destroy the Planet"
run/main_scene="res://Splash.tscn"
config/icon="res://icon.png"

[display]

window/size/width=1280
window/size/height=720
window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[layer_names]

2d_physics/layer_1="none"
2d_physics/layer_2="placing_area"
2d_physics/layer_3="preview_icon"
2d_physics/layer_4="plant"
2d_physics/layer_5="building"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0, 0.733333, 1, 1 )
environment/default_environment="res://default_env.tres"
